#include<stdio.h>
#include<windows.h>
COORD coord = {0, 0};
void gotoxy (int x, int y)
{
coord.X = x; coord.Y = y;
SetConsoleCursorPosition(GetStdHandle(STD_OUTPUT_HANDLE), coord);
}
int main()
{
    system("COLOR FC");
    gotoxy(2,4);
    Sleep(50);
    printf("\xdb\xdb");
    gotoxy(2,8);
    Sleep(50);
